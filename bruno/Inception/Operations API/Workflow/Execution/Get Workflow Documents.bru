meta {
  name: Get Workflow Documents
  type: http
  seq: 33
}

get {
  url: {{baseURL}}/api/operations/workflows/{{workflowId}}/documents
  body: none
  auth: inherit
}

vars:post-response {
  documentId: res.body.workflowDocuments[1].documentId
}

assert {
  res.status: eq 200
}

settings {
  encodeUrl: true
}
