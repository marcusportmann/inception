meta {
  name: Get Workflow Documents
  type: http
  seq: 3
}

get {
  url: {{baseURL}}/api/operations/workflows/{{workflowId}}/documents
  body: none
  auth: inherit
}

vars:post-response {
  workflowDocumentId: res.body.workflowDocumentSummaries[0].id
}

assert {
  res.status: eq 200
}

settings {
  encodeUrl: true
  timeout: 0
}
