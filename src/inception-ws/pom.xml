<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/POM/4.0.0"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

  <artifactId>inception-ws</artifactId>

  <build>
    <plugins>
      <plugin>
        <artifactId>maven-deploy-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>deploy</goal>
            </goals>
            <id>default-deploy</id>
            <phase>deploy</phase>
          </execution>
        </executions>
        <groupId>org.apache.maven.plugins</groupId>
      </plugin>
      <plugin>
        <artifactId>maven-enforcer-plugin</artifactId>
        <executions>
          <execution>
            <configuration>
              <rules>
                <dependencyConvergence>
                  <excludes>
                    <exclude>com.google.guava:guava:[31,34)</exclude>
                    <exclude>org.apache.santuario:xmlsec:[2,4)</exclude>
                    <exclude>org.bouncycastle:bcpkix-jdk18on:[1,2)</exclude>
                    <exclude>org.bouncycastle:bcprov-jdk18on:[1,2)</exclude>
                    <exclude>org.cryptacular:cryptacular:[1,2)</exclude>
                  </excludes>
                </dependencyConvergence>
              </rules>
            </configuration>
            <goals>
              <goal>enforce</goal>
            </goals>
            <id>enforce</id>
          </execution>
        </executions>
        <groupId>org.apache.maven.plugins</groupId>
      </plugin>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
        <configuration>
          <failOnWarnings>true</failOnWarnings>
          <quiet>true</quiet>
        </configuration>
        <executions>
          <execution>
            <goals>
              <goal>jar</goal>
            </goals>
            <id>attach-javadocs</id>
          </execution>
        </executions>
        <groupId>org.apache.maven.plugins</groupId>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <!-- Inception Dependencies -->
    <dependency>
      <artifactId>inception-core</artifactId>
      <groupId>digital.inception</groupId>
    </dependency>

    <!-- Dependencies -->
    <dependency>
      <artifactId>cxf-rt-frontend-jaxws</artifactId>
      <groupId>org.apache.cxf</groupId>
    </dependency>
    <dependency>
      <artifactId>cxf-rt-transports-http</artifactId>
      <groupId>org.apache.cxf</groupId>
    </dependency>
    <dependency>
      <artifactId>cxf-rt-ws-security</artifactId>
      <groupId>org.apache.cxf</groupId>
    </dependency>
    <dependency>
      <artifactId>wss4j-ws-security-common</artifactId>
      <groupId>org.apache.wss4j</groupId>
    </dependency>

    <!-- Provided Dependencies -->
    <dependency>
      <artifactId>jakarta.servlet-api</artifactId>
      <groupId>jakarta.servlet</groupId>
      <optional>true</optional>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <artifactId>spring-boot</artifactId>
      <groupId>org.springframework.boot</groupId>
      <optional>true</optional>
      <scope>provided</scope>
    </dependency>
  </dependencies>

  <modelVersion>4.0.0</modelVersion>

  <name>inception-ws</name>

  <parent>
    <artifactId>inception-parent</artifactId>
    <groupId>digital.inception</groupId>
    <version>1.0.0-SNAPSHOT</version>
  </parent>

</project>


